    <script language="JavaScript">
  function refreshimg2()
  {
    document.getElementById('newimg2').src="http://ptlogin2.qq.com/getimage?"+Math.random();
  }
  </script>
  <script type="text/javascript">
    function OnSearchIpSubmit()
  {
    document.getElementById('searchip1').value = trim(document.getElementById('searchip1').value);
    if(document.getElementById('searchip1').value=="")
    {
      alert('IP地址不能为空');
      return false;
    }
    else if(document.searchip.verifycode2.value == "")
    {
      alert("您还没有输入验证码!");
      form.verifycode.focus();
      return false;
    }
    else if(document.searchip.verifycode2.value.length != 4)
    {
      alert("您输入的验证码有误！");
      form.verifycode.focus();
      return false;
    }
    else
    {
      var pattern = /^\d{1,3}(\.\d{1,3}){3}$/;
      if(!pattern.exec(document.getElementById('searchip1').value))
      {
        alert('请输入正确的地址\n');
        document.getElementById('searchip1').value="";
        document.searchip.searchip1.focus();
        return false
      }
      document.searchip.submit();
    }
  }
  </script>
