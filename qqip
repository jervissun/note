<SCRIPT LANGUAGE="JavaScript">
var g_uin=0;
var g_show=0;
var filterXSS = function(e) {

    if (!e) return e;

    for (; e != unescape(e);) e = unescape(e);

    for (var r = ["<", ">", "'", '"', "%3c", "%3e", "%27", "%22", "%253c", "%253e", "%2527", "%2522"], n = ["&#x3c;", "&#x3e;", "&#x27;", "&#x22;", "%26%23x3c%3B", "%26%23x3e%3B", "%26%23x27%3B", "%26%23x22%3B", "%2526%2523x3c%253B", "%2526%2523x3e%253B", "%2526%2523x27%253B", "%2526%2523x22%253B"], a = 0; a < r.length; a++) e = e.replace(new RegExp(r[a], "gi"), n[a]);

    return e

};
function login_in(uin)
{
  if(uin!=0 || getUin()!="" )
  {
    if(uin!=0)
      g_uin=uin;
    //document.getElementById('login_index').style.visibility="visible";
    //document.getElementById('login_uin').style.visibility="visible";
    //document.getElementById('login_field').style.display="none";
    document.getElementById('login_uin').innerHTML="您登录的QQ号码："+filterXSS(uin);
    document.getElementById('log_off').innerHTML="<a class=\"exit\" style=\"cursor:pointer\" onclick='javascript:if(confirm(\"确实要取消登陆吗?\")){delCookie();}' href='#'>[退出]</a>";
    if(g_show==1)
      Change_Hide_bak(0);
  }
  else
  {
    g_uin=uin;
  }

  return false;
}

function ShowIndex(index)
{
  document.getElementById('search_show').style.display=index?"block":"none";
  document.getElementById('login_show').style.display=index?"none":"block";
  return false;
}
function OnSubmit()
{
  var flag=1,errmsg="";

  if( document.shareip.country.value=="中国" && document.shareip.province.value=="省份" && document.shareip.city.value=="地级市" && document.shareip.town.value=="市、县级市、县")
  {
    flag=0;
    errmsg=errmsg+"您还没有选择该IP所在地区\n";
  }
  if(flag==1)
  {
    if(document.shareip.country.value!="中国")
    {
      document.shareip.province.value="";
      document.shareip.city.value="";
      document.shareip.town.value="";
    }

    document.shareip.province.value=document.shareip.province.value.replace("省份","");
    document.shareip.city.value=document.shareip.city.value.replace("地级市","");
    document.shareip.town.value=document.shareip.town.value.replace("市、县级市、县","");
    document.shareip.submit();
  }
  else
  {
    alert(errmsg);
    return false
  }
}

void function () {
        document.getElementById('bkn').value = getBkn(getCookie('skey'));

        function getCookie(k){

            var filterXSS = function(e) {
                if (!e) return e;
                for (; e != unescape(e);) e = unescape(e);
                for (var r = ["<", ">", "'", '"', "%3c", "%3e", "%27", "%22", "%253c", "%253e", "%2527", "%2522"], n = ["&#x3c;", "&#x3e;", "&#x27;", "&#x22;", "%26%23x3c%3B", "%26%23x3e%3B", "%26%23x27%3B", "%26%23x22%3B", "%2526%2523x3c%253B", "%2526%2523x3e%253B", "%2526%2523x27%253B", "%2526%2523x22%253B"], a = 0; a < r.length; a++) e = e.replace(new RegExp(r[a], "gi"), n[a]);
                return e
            };

            var a;
            return filterXSS((a=document.cookie.match(RegExp("(^|;\\s*)"+k+"=([^;]*)(;|$)")))?unescape(a[2]):null)
        }

        function getBkn(skey) {
            if (!skey) { return ""; }
            var hash = 5381;
            for (var i = 0, len = skey.length; i < len; ++i) {
                hash += (hash << 5) + skey.charAt(i).charCodeAt();
            }
            return hash & 0x7fffffff;
        }
    }();
</SCRIPT>
